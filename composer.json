{
  "name": "dcarbone/php-consul-api",
  "type": "library",
  "description": "PHP client for the Consul HTTP API",
  "homepage": "https://github.com/dcarbone/php-consul-api",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Daniel Carbone",
      "email": "daniel.p.carbone@gmail.com"
    }
  ],
  "keywords": [
    "consul",
    "consul-api",
    "php-consul-api"
  ],
  "require": {
    "php": "^7.0",
    "guzzlehttp/guzzle": "~6",
    "guzzlehttp/psr7": "~1"
  },
  "autoload": {
    "files":[
      "src/init.php",
      "src/Coordinate/funcs.php"
    ],
    "psr-4": {
      "DCarbone\\PHPConsulAPI\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0"
  },
  "autoload-dev": {
    "files":[
      "src/init.php",
      "src/Coordinate/funcs.php"
    ],
    "psr-4": {
      "DCarbone\\PHPConsulAPI\\": "src/",
      "DCarbone\\PHPConsulAPITests\\": "tests/"
    }
  }
}